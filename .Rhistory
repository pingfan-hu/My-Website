library(tidyverse)
library(kableExtra)
region <- tibble(`No.` = seq(1,8),
`Abbr.` = c('DC', 'DE', 'MD', 'NC', 'NJ', 'NY', 'PA', 'VA'),
`Full Name` = c('District of Columbia', 'Delaware',
'Maryland', 'North Carolina', 'New Jersey',
'New York', 'Pennsylvania', 'Virginia'))
region_kable <- region %>% kable(
format = 'html', escape = FALSE,
align = c('c', 'c', 'l'),
caption = 'The 8 states of the Mid-Atlantic region'
) %>% kable_styling(bootstrap_options = c('striped', 'hover'))
